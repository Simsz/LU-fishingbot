{
  "name": "LU Fish Bot",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs", "tabCapture", "notifications"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "media/Icon16.png",
    "32": "media/Icon32.png",
    "48": "media/Icon48.png",
    "128": "media/Icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
